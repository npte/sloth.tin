#var autostrike $false
#alias {autostrike} {
    #if {"$autostrike" == "$false"} {
        #var autostrike $true;
        #class AUTOSTRIKE read autostrike.tin;
    } {
        #var autostrike $false;
        #class AUTOSTRIKE kill;
    };
    #echo {Autostrike $autostrike};
}

#var autoblast $false
#alias {autoblast} {
    #if {"$autoblast" == "$false"} {
        #var autoblast $true;
        #class AUTOBLAST read autoblast.tin;
    } {
        #var autoblast $false;
        #class AUTOBLAST kill;
    };
    #echo {Autoblast $autoblast};
}

#var autoheal $false
#alias {autoheal} {
    #if {"$autoheal" == "$false"} {
        #if {"$autoblast" == "$false"} {
            #var autoblast $false;
            #class AUTOBLAST kill;
        };
        #var autoheal $true;
        #class AUTOHEAL read autoheal.tin;
    } {
        #var autoheal $false;
        #class AUTOHEAL kill;
    };
    #echo {Autoheal $autoheal};
}

