#alias trg {
	#var target %1;
	#if {MSDP_CHARACTER_NAME == MSDP_GROUP_LEADER} {
		#send {gt target $target};
	};
};

#alias {was} {
	wake;
	stand;
};

#alias {cfood} {
	cast 'create food';
	get food;
	eat food;
};

#alias {cwater} {
	cast 'create water' $watercontainer;
	drink $watercontainer;
};

#action {You are thirsty} {
	#if {$checkarenaenter == "1"} {
		wake;
		stand;
		cast 'create water' $watercontainer;
		dri $watercontainer;
		sle;
	};
	#if {$checkarenaenter == "0"} {
		dri $watercontainer;
	};
};

#action {You are hungry} {
	#if {$checkarenaenter == "1"} {
		wake;
		stand;
		cast 'create food';
		get food;
		eat food;
		sle;
	};
};

